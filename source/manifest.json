{
  "manifest_version": 2,
  "version": "1.0",
  "name": "SchoolNotesCleaner",
  "description": "Deletes gray-ed out portions",
  "browser_action": {
    "default_icon": "icon.png"
    }


    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ]
}
